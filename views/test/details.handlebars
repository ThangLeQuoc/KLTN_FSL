	<div class="panel-heading">Information</div>
	<div class="panel-body">



		<div class="form-group">
			<label class="col-sm-2 control-label">Id: </label>
			<div class="col-sm-10">
				<label>{{doc._id}}</label>
			</div>
		</div>
		<div class="form-group">
			<label class="col-sm-2 control-label">Giá phòng: </label>
			<div class="col-sm-10">
				<label>{{doc.giaphong}}</label>
			</div>
		</div>
		<div class="form-group">
			<label class="col-sm-2 control-label">Giá điện:</label>
			<div class="col-sm-10">
				<label>{{doc.giadien}}</label>
			</div>
		</div>
		<div class="form-group">
			<label class="col-sm-2 control-label">Giá nước:</label>
			<div class="col-sm-10">
				<label>{{doc.gianuoc}}</label>
			</div>
		</div>
		<div class="form-group">
			<label class="col-sm-2 control-label">Mô tả : </label>
			<div class="col-sm-10">
				<label>{{doc.motacackhoangiakhac}}</label>
			</div>
		</div>
